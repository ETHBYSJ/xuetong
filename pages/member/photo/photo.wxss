.base-nav-grid {
   display: inline-flex;
  justify-content: flex-start;
  flex-wrap: wrap;
  text-align: center;
   margin-left: 12rpx;
}
.base-nav-grid-pic {
  color: #989cb5;
  margin-left: 8rpx;

}
.base-nav-grid-pic image{
  width: 230rpx;
  height: 230rpx;
}
.base-nav-grid-pic video{
  width: 230rpx;
  height: 230rpx;
}
.upload_btn {
   width: 230rpx;
  height: 230rpx;
  line-height: 230rpx;
  color: #1296db;
  border: 3rpx solid #1296db;
  font-size: 60rpx;
}
.attachLists {
  width: 88rpx;
  height: 88rpx;
  margin-left: 20rpx;
  padding: 2rpx;
  border:2rpx solid #fff;
}
.uploadPhoto{
  display: inline-block;
  position: absolute;
  top:28rpx;
  right: 20rpx;
  width: 60rpx;
  height: 60rpx;
}
.uploadVideo{
  display: inline-block;
  position: absolute;
  top:28rpx;
  right: 120rpx;
  width: 56rpx;
  height: 56rpx;
}
.title-warper{
  width: 100%;
  position: absolute;
  top:0;
  left:0;
  height: 115rpx;
  background: #1296db;
  color: #fff;
  z-index: 99;
}
.controls{
  position: relative;
   cursor:pointer;
   top: 0rpx;
   left: 0rpx;
   height: 230rpx;
   width: 230rpx;
}
.video_image{
  position: relative;
  width: 100rpx;
  height: 100rpx;
  margin: 0 auto;
  top:-165rpx;
  z-index: 98;
}
.video_backgroud{
  position: relative;
  width:128rpx;
   margin: 0 auto;
  
}
.video_test{
  position: relative;
  color: #fff;
  font-size: 30rpx;
  width: 100rpx;
  margin: 0 auto;
  top:-160rpx;
  z-index: 99;
}