swiper{
  height: 100vh;
  background:#F0F4F3;
}
.member{
  width: 710rpx;
  height: 226rpx;
  margin: 15rpx auto;
  margin-bottom: 30rpx;
  background: #1296db;
  border-radius: 20rpx;
  position: relative;
}
.memberpic{
  width: 115rpx;
  height: 115rpx;
  overflow: scroll;
  border-radius: 50%;
  margin-left: 30rpx;
  margin-top: 55rpx;
  float: left;
}
.nickname{
  margin-top:20rpx; 
  float: left;
  width: 540rpx;
}
.Loginname{
  font-size: 26rpx;
  color: #fff;
  float: right;
  width: 520rpx;
  margin-top: 35rpx;
  height: 40rpx;
  line-height: 40rpx;
}
.Loginname .left{
  display: inline-block;
  margin-top:20rpx;
  margin-right: 30rpx;
  width:210rpx; 
}
.Loginname .right{
  display: inline-block;
   vertical-align:top;
   padding-top:20rpx;
   width:140rpx;
   
}
.check{
  margin: 0 auto;
  width: 100%;
  height: 124rpx;
  text-align: left;
  color:#000000;
  font-size: 32rpx;
  position: relative;
}
.check-nav{
	position: absolute;
	width:100%;
	height: 124rpx;
}
.base-cell__ft_in-access:after {
    content: " ";
    display: inline-block;
    height: 12rpx;
    width: 12rpx;
    border-width: 4rpx 4rpx 0 0;
    border-color: #8a8a8a;
    border-style: solid;
    -webkit-transform: matrix(.71, .71, -.71, .71, 0, 0);
    transform: matrix(.71, .71, -.71, .71, 0, 0);
    position: relative;
    top: -2px;
    position: absolute;
    top: 50%;
    margin-top: -50rpx;
    right: 30rpx
}

.check-icon{
  width:32rpx;
  height:32rpx;
  position: relative;
  bottom: -2rpx;
  margin-left: 20rpx;
  margin-right: 10rpx;
}
.check-time{
  height: 114rpx;
  color: #000000;
   background: #fff;
}

.date{
  margin-left: 20rpx;
  margin-top: 30rpx;
  height: 52rpx;
  border-radius: 20rpx;
  background: #1296db;
  color: #fff;
  display:inline-block;
  vertical-align: top;
  padding: 0 10rpx
}
.time-wrap{
  display:inline-block;
  font-size: 26rpx;
  color:#8a8a8a;
}
.time{
  padding-top:10rpx;
  padding-left: 40rpx;
}
.greadtitle{
  display: inline-block;
  vertical-align:top;
padding-top:20rpx;
padding-right:4rpx;

}
