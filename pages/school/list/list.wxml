<view class="page">
     <view class="base-list-film">
 
    <view class="base-list-film-details">
      <view class="chioce-tit">{{vo.cinemaDetails.name}}</view>
      <view class="chioce-item last-list" bindtap="openMap">
        <image class="base-cinema-loaction-img" src="/assets/icon/location-fill.png" mode='widthFix'></image>
        <text>{{vo.cinemaDetails.address}}</text>
      </view>
       <view class="chioce-item">
        <image class="base-cinema-loaction-img" src="/assets/icon/dashboard-fill .png" mode='widthFix'></image>
        <text>营业时间：{{vo.cinemaDetails.worktime}}</text>
      </view>
       <view class="chioce-item last-list" bindtap='calling'>
        <image class="base-cinema-loaction-img" src="/assets/icon/phone-fill.png" mode='widthFix'></image>
        <text>{{vo.cinemaDetails.phone}}</text>
      </view>
       <view class="chioce-item">
        <image class="base-cinema-loaction-img" src="/assets/icon/image-fill.png" mode='widthFix'></image>
        <text>门店照片</text>
      </view>
    </view>
  </view>
  <view hidden="{{vo.roomList==0}}" class="tab-content">
    <view class='base-nav-grid'>
      <view class='base-nav-grid-pic' wx:for="{{vo.roomList}}" wx:for-item="dImg">
        <image src='{{dImg}}' bindtap="imgYu" data-list="{{vo.roomList}}" data-src="{{dImg}}"></image>
      </view>
    </view>
  </view>

   <view class='bottombut'>
 
     <button class="base-btn left {{Wantseecss}}" bindtap='Wantsee' data-info="{{vo.getMovie.id}}">
        <image src='{{vo.Wantseeimage}}'></image>    点赞</button>
      <button class="base-btn cer" bindtap='onShareAppMessage' open-type="share">分享给其他朋友</button>
 
  </view>
</view>