.last-list:after {
    content: " ";
    display: inline-block;
    height: 10rpx;
    width: 10rpx;
    border-width: 4rpx 4rpx 0 0;
    border-color: #000;
    border-style: solid;
    -webkit-transform: matrix(.71, .71, -.71, .71, 0, 0);
    transform: matrix(.71, .71, -.71, .71, 0, 0);
    position: relative;
    top: 0%;
    margin-bottom: 5rpx;
    left: 100rpx;
}

