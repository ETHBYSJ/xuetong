/*搜索框*/
.base-search-bar{
  background: #0f1024;
  border: 0;
}
.base-search-bar__form{
  border:2rpx solid #3e4260;
  background:  #3e4260;
}
.base-search-bar__box,.base-search-bar__label{
  background: #3e4260;
}
.base-icon-search{
  position: relative;
  top:1px;
  }
.base-search-bar__text{
   position: relative;
   top:-1px;
   font-size:26rpx; 
  }
  .base-icon-search_in-box{
    top: 16rpx;
  }
  .base-search-bar__input{
    font-size:26rpx; 
    color: #bcbdc0;
  }
  .base-search-bar__cancel-btn{
    font-size: 28rpx;
    color: #c0c0c0;
    margin-top:2rpx; 
  }
  .searchbar-result{
    position:absolute;
    width: 100%;
    background: #0f1024;
    color: #c0c0c0;
    border-color: #c0c0c0;
    font-size: 26rpx;
    z-index: 5;
    margin-top:0; 
  }
  .base-cells:before{
    border-top: 2rpx solid #3e4260;
  }
  .base-cells:after {
    border-bottom: 2rpx solid #3e4260
  }
  .base-cell:before{
    border-top: 2rpx solid #3e4260;
    left: 0;
  }


