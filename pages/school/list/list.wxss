.base-searchcon-size{
  width: 100%;
  display: flex;
  justify-content: space-around;
  margin: 0 auto;
  padding: 20rpx 0;
  color: #f00;
  background: #2d304b;
}
.base-searchcon-size .listItem{
  width: 20%;
  height: 50rpx;
  text-align: center;
  box-sizing: border-box;
  line-height: 50rpx;
  border: 2rpx solid #686c81;
 
  position: relative;
  border-radius: 5rpx;
 
}
.base-searchcon-size .listItem text{
  display: block;
  font-size: 21rpx;
  color: #c0c0c0;
}
.base-searchcon-size .current{
  border: 2rpx solid #DF5924;
  z-index: 1;
}
.base-searchcon-size .current text{
  color: #DF5924;
}

.base-searchcon-size .listItem image{
  width: 20rpx;
  height: 20rpx;
  opacity: 0;
  position: absolute;
  top: 12rpx;
  left: 10rpx;
 
}
.base-searchcon-size .listItem.current image{
  width: 20rpx;
  height: 20rpx;
  opacity: 1;
  position: absolute;
  top: 12rpx;
  left: 10rpx;
}
