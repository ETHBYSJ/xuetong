.title{
  width:100%;
  position: fixed;
   z-index: 11;
  left: 0;
  top:0;
  display: flex;
  text-align: center;
  color: #fff;
  background: #1296db;
  height: 80rpx;
  line-height: 80rpx;

}
.title-btn{
  flex: 1;
}
.FilePath{
  position: absolute;
  top:100rpx;
  z-index: 10;
  margin: 0 auto;
}
.cube{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}
.cube view{
  position: absolute;
  background: RGBA(0,0,0,0.5);
}
.cube view:nth-child(1){
  height: 30%;
  width: 100%;
  top: 0;
  left: 0;
}
.cube view:nth-child(2){
  height: 30%;
  width: 30%;
  top: 30%;
  left: 0;
}
.cube view:nth-child(3){
  height: 30%;
  width: 30%;
  top: 30%;
  right: 0;
}
.cube view:nth-child(4){
  height: 40%;
  width: 100%;
  bottom: 0;
  right: 0;
}